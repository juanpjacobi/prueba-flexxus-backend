{
    "version": 2,
    "builds": [
      {
        "src": "api/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  